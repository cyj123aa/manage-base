<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hoolink.manage.base.dao.mapper.ext.MiddleRoleMenuMapperExt">

  <insert id="bulkInsert" parameterType="java.util.List">
    insert into middle_role_menu (role_id, menu_id, permission_flag)
    values
    <foreach collection="middleRoleMenus" item="menu" separator=",">
      (#{menu.roleId},#{menu.menuId},#{menu.permissionFlag})
    </foreach>
  </insert>

</mapper>